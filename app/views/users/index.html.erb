<h1><u>Welcome to Tacostagram</u></h1>

<h1>Places</h1>

<div class="container-float">
  <div class="row">
    <% for place in @places %>
      <div class="col-md-4 d-flex align-items-stretch">
        <div class="card mb-4 w-100">
          <div class="card-body d-flex flex-column justify-content-center text-center">
            <a href="/sessions/new" class="btn btn-light btn-sm"><%= place["name"] %></a>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

<p>
  <a class="btn btn-primary" href="/sessions/new" role="button">Go to Login</a>
  <a class="btn btn-primary" href="/users/new" role="button">Sign Up</a>
</p>
